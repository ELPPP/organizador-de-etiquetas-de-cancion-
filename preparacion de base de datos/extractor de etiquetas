from os import walk
import xlsxwriter as xw
ruta=r"S:\\Documentos\\Programacion\\Proyecto ordenar musica con Pyton e IA\\preparacion de base de datos\\canciones.xlsx"
print(ruta,"mojarra")
archivo=xw.Workbook(ruta)
hoja=archivo.add_worksheet()
print("archivo creado")
ruta = 'S:\Musica\Musica\salvar.flac'
h=walk(ruta)
k=2
p=604
for n in h:
    j=n[k]
for y in j:
    hoja.write(p,1,y)
    print("he a√±adido ",y," en la fila: ",p)
    p+=1
archivo.close()